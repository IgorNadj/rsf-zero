{
  "name": "rsf-zero",
  "description": "A minimal micro-framework with React Server Functions support",
  "author": "Igor Nadj",
  "type": "module",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/IgorNadj/rsf-zero.git",
    "directory": "rsf-zero"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build && npm run test && cp ../README.md ."
  },
  "dependencies": {
    "@swc/core": "^1.12.1",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "superjson": "^2.2.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/express": "^5.0.3",
    "@types/md5": "^2",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.1.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "exports": {
    "./client": {
      "types": "./dist/lib/client.d.ts",
      "default": "./dist/lib/client.js"
    },
    "./server": {
      "types": "./dist/export-types.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "packageManager": "yarn@4.6.0",
  "engines": {
    "node": ">=22.14.0"
  },
  "bin": "dist/cli.js"
}
